{
  "extends": "eslint:recommended",
  "env": {
     "es6": true,
     "browser": true,
     "node": true,
     "jest": true
   },
  "rules": {
    "no-unused-vars": "warn",
    "no-param-reassign": ["error", { "props": false }],
    "no-restricted-globals": ["error", "event", "fdescribe"],
    "no-restricted-syntax": [
      "error",
      "LabeledStatement",
      "WithStatement"
    ]
  }
}